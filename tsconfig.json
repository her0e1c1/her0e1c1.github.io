{
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "declaration": false,
    "noResolve": false,
    "allowJs": false,
    "jsx": "react",
    "module": "commonjs",
    "target": "es5",
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "."
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*"
  ]
}
